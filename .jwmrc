<Key mask="4" key="d">exec:STATUS=`xset -q | grep -e 'DPMS is'| cut -c 10-17`; if  [ "$STATUS" = " Disable" ]]; then $(xset s on +dpms; echo "Zapinam DPMS ...setrim"  | osd_cat -d 1 -s 1 -A right -l 1 -p middle -o 300 -c green -f '-adobe-helvetica-bold-r-normal--34-240-100-100-p-182-iso8859-1'); elif [ "$STATUS" = " Enabled" ]] ;then $(xset s on -dpms ;echo "Vypinam DPMS ...nesetrim"  | osd_cat -d 1 -s 1 -A right -l 1 -p middle -o 300 -c green -f '-adobe-helvetica-bold-r-normal--34-240-100-100-p-182-iso8859-1');  fi #vypne usporu energie DPMS - switch zapne/vypne</Key>
