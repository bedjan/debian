<Menu  label="Systemove upravy">
<Program label="zadat a najit balicek k instalaci, prohlizeni posuvnikem">urxvt -geometry 800x600+60+150 -title "zadat a najit balicek  k instalaci" -hold -e bash -c ' echo "zadej balicek ktery hledas  k instalaci"; read -n 100 -t 10 PROMENA ; apt-cache search "$PROMENA" #zadat a najit balicek'</Program>

<Program label="zadat a zobrazit informace o balicku">urxvt -geometry 800x600+60+150 -title "zadat a zobrazit informace o balicku" -hold -e bash -c 'echo "zadej balicek ktery hledas";apt-cache show $(read -n 100 -t 10 PROMENA ; echo $PROMENA) #zadat a zobrazit informace o balicku'</Program>
<Program label="zadat a nainstalovat balicek bez zavislosti">urxvt -geometry 800x600+60+150 -title "zadat a nainstalovat balicek bez zavislosti" -hold -e bash -c 'echo "zadej balicek ktery hledas";sudo apt-get install --no-install-recommends $(read -n 100 -t 10 PROMENA ; echo $PROMENA) #zadat a nainstalovat balicek bez zavislosti'</Program>
<Program label="zadat a nainstalovat balicek se zavislostmi">urxvt -geometry 800x600+60+150 -title "zadat a nainstalovat balicek se zavislostmi" -hold -e bash -c 'echo "zadej balicek ktery chces nainstalovat";sudo apt-get install $(read -n 100 -t 10 PROMENA ; echo $PROMENA) #zadat a nainstalovat balicek '</Program>
<Program label="zadat a odstranit balicek">urxvt -geometry 800x600+60+150 -title "zadat a odstranit balicek" -hold -e bash -c 'echo "zadej balicek ktery chces odstanit";sudo apt-get remove $(read -n 100 -t 10 PROMENA ; echo $PROMENA) #zadat a odstranit balicek'</Program>
<Program label="vypise vsechny nainstalovane balicky  do souboru dpkgl">dpkg -l | awk '{ print $2 }' | tr -s '\n' ' ' | sed 's/:i386//g' >dpkgl &amp;&amp; urxvt -geometry 800x600+60+150 -title "vypise vsechny nainstalovane balicky  do souboru dpkgl" -hold -e cat dpkgl #vypise vsechny nainstalovane balicky do souboru dpkgl</Program>
<Program label="zadat a najit nainstalovany balicek">urxvt -geometry 800x600+60+150 -title "zadat a najit nainstalovany balicek" -hold -e bash -c 'echo "zadej vyraz ktery hledas v historii";read -n 100 -t 10 PROMENA ; echo "hledam $PROMENA"; dpkg -l | grep "$PROMENA" #zadat a najit nainstalovany balicek'</Program>
<Program label="zadat a najit v historii zadavany prikaz">urxvt -geometry 800x600+60+150 -title "zadat a najit v historii zadavany prikaz" -hold -e bash -c 'read -n 100 -t 10 PROMENA ; echo "hledam $PROMENA"; history | grep "$PROMENA" #zadat a najit v historii zadavany prikaz'</Program>
<Program label="vycisti nepotrebne balicky">urxvt -geometry 800x600+60+150 -title "vycisti nepotrebne balicky, nic vic neni potreba zadavat" -hold -e bash -c 'echo "automaticky mazu nepotrebne balicky";sudo apt-get autoremove #vycisti nepotrebne balicky'</Program>
<Program label="chyby grafickeho serveru Xorg">urxvt -geometry 800x600+60+150 -title "chyby grafickeho serveru Xorg" -hold -e bash -c "egrep '\(EE\)|\(WW\)' /var/log/Xorg.1*"</Program>

