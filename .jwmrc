<Menu  label="Prehravani z webu = URL ve schrance a spustit">
<Program label="vlc prehraje zvuk,video z url adresy, vlozit adresu zkopirovanim do schranky">rxvt -geometry 80x20+60+150 -title "vlc prehraje zvuk,video z url adresy, vlozit adresu zkopirovanim do schranky" -hold -e bash -c 'echo "prehravam vlc adresu $(xsel --clipboard)"; vlc --playlist-autostart --loop --playlist-tree  "$(xsel --clipboard)"  #vlc prehraje zvuk,video z url adresy'</Program>

<Program label="mplayer prehraje zvuk,video z url adresy, vlozit adresu zkopirovanim do schranky">rxvt -geometry 80x20+60+150 -title "mplayer prehraje zvuk,video z url adresy, vlozit adresu zkopirovanim do schranky" -hold -e bash -c 'echo "prehravam mplayerem adresu $(xsel --clipboard)";mplayer "$(xsel --clipboard)" #mplayer prehraje zvuk,video z url adresy'</Program>

<Program label="stahnout video z url adresy, vlozit adresu zkopirovanim do schranky">rxvt -geometry 80x20+60+150 -title "stahnout youtube-dl video z url adresy, vlozit adresu zkopirovanim do schranky" -hold -e bash -c 'echo "stahuji video z adresy $(xsel --clipboard)";youtube-dl "$(xsel --clipboard)" '</Program>

</Menu>

